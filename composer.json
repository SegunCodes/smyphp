    },
    "repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/SegunCodes/smyphp"
		}
	],
    "authors": [
        {
            "name": "Olusegun Joe-Alabi",
            "email": "shegstix64@gmail.com",
            "homepage": "https://twitter.com/dreal_olusegun"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "phpmailer/phpmailer": "^6.6",
        "firebase/php-jwt": "^6.3",
        "filp/whoops": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
